{
  "name": "react-style-component-helper",
  "displayName": "react-style-component-helper",
  "version": "1.0.2",
  "publisher": "NoamDadon",
  "description": "Help you build styled component with fixed format and less boiler code",
  "author": {
    "name": "Noam Dadon"
  },
  "icon": "iconS.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.71.0"
  },
  "activationEvents": [
    "onCommand:react-style-component-helper.generateStyled"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-style-component-helper.generateStyled",
        "title": "Generate styled"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DadonStyle/react-style-component-helper.git"
  },
  "license": ""
}
